{
  "expo": {
    "name": "BizKhoj",
    "slug": "bizkhoj",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/bizkhoj.png",
    "scheme": "bizkhoj",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "updates":{
      "url": "https://u.expo.dev/ce278195-c2cd-49a4-af05-b8097d7dba4c",
      "fallbackToCacheTimeout": 0
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF",
        "foregroundImage": "./assets/images/bizkhoj.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/bizkhoj.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "statusBar": {
            "style": "dark"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ce278195-c2cd-49a4-af05-b8097d7dba4c"
      }
    },
    "owner": "ankitkhanal-2"
  }
}
